local A=game:GetService("Players")local B=game:GetService("Workspace")local C=A.LocalPlayer local D=B.CurrentCamera local E=false local F local G=1e-12 local H=false local I=UDim2.new(0.5,-80,0,20)local J=false local K={["StuddedKillPart"]=true,["TeleportPart"]=true,["무지개 파트"]=true,["giver"]=true,["Block"]=true}local function L()if H then return end H=true pcall(function()game.StarterGui:SetCore("SendNotification",{Title="Script Fire Block",Text="BY AMDZY_1",Duration=5})end)end local function M()if F then I=F:FindFirstChild("Frame")and F.Frame.Position or I F:Destroy()end F=Instance.new("ScreenGui",C:WaitForChild("PlayerGui"))F.Name="FIRE_GUI"local N=Instance.new("Frame")N.Name="Frame"N.Size=UDim2.new(0,160,0,100)N.Position=I N.BackgroundColor3=Color3.fromRGB(0,0,0)N.BorderSizePixel=2 N.Active=true N.Draggable=true N.Parent=F local O=Instance.new("TextLabel")O.Size=UDim2.new(1,-20,0,20)O.Text="Fire Block BY AMDZY_1"O.TextColor3=Color3.fromRGB(255,255,255)O.BackgroundColor3=Color3.fromRGB(30,30,30)O.Font=Enum.Font.SourceSansBold O.TextSize=13 O.TextXAlignment=Enum.TextXAlignment.Left O.Parent=N local P=Instance.new("TextButton")P.Size=UDim2.new(0,20,0,20)P.Position=UDim2.new(1,-20,0,0)P.Text="-"P.TextColor3=Color3.new(1,1,1)P.BackgroundColor3=Color3.fromRGB(50,50,50)P.Font=Enum.Font.SourceSansBold P.TextSize=14 P.Parent=N local Q=Instance.new("TextButton")Q.Size=UDim2.new(1,0,0,35)Q.Position=UDim2.new(0,0,0,22)Q.Text=E and"OFF FIRE"or"ON FIRE"Q.TextColor3=Color3.fromRGB(255,255,255)Q.BackgroundColor3=E and Color3.fromRGB(200,0,0)or Color3.fromRGB(0,200,0)Q.Font=Enum.Font.SourceSansBold Q.TextSize=16 Q.Parent=N local R=Instance.new("TextBox")R.Size=UDim2.new(1,-10,0,20)R.Position=UDim2.new(0,5,0,62)R.Text=tostring(G)R.PlaceholderText="Cooldown (detik)"R.TextColor3=Color3.new(1,1,1)R.BackgroundColor3=Color3.fromRGB(40,40,40)R.Font=Enum.Font.SourceSans R.TextSize=14 R.ClearTextOnFocus=false R.Parent=N R.FocusLost:Connect(function()local S=tonumber(R.Text)if S and S>=1e-9 and S<=10 then G=S else R.Text=tostring(G)end end)Q.MouseButton1Click:Connect(function()E=not E Q.Text=E and"OFF FIRE"or"ON FIRE"Q.BackgroundColor3=E and Color3.fromRGB(200,0,0)or Color3.fromRGB(0,200,0)end)P.MouseButton1Click:Connect(function()J=not J if J then for _,T in ipairs(N:GetChildren())do if T~=O and T~=P then T.Visible=false end end N.Size=UDim2.new(0,160,0,20)P.Text="+"else for _,T in ipairs(N:GetChildren())do T.Visible=true end N.Size=UDim2.new(0,160,0,100)P.Text="-"end end)L()end local function U(V)if not V:IsA("BasePart")then return false end if K[V.Name]then return false end if not(V.Name:match("^Gudock")or V.Name=="사라지는 파트")then return false end if not V:FindFirstChildOfClass("TouchTransmitter")then return false end for _,W in ipairs(V:GetDescendants())do if W:IsA("Script")or W:IsA("LocalScript")then return true end end return false end local function X()local Y=C.Character if not Y or not Y:FindFirstChild("HumanoidRootPart")then return end local Z=Y.HumanoidRootPart for _,a0 in ipairs(B:GetDescendants())do if U(a0)then firetouchinterest(Z,a0,0)end end task.wait(0.01)for _,a0 in ipairs(B:GetDescendants())do if U(a0)then firetouchinterest(Z,a0,1)end end end task.spawn(function()while true do if E then pcall(X)end task.wait(G)end end)M()C.CharacterAdded:Connect(function()E=false task.wait(1)M()end)
